# Test Environment Configuration
NODE_ENV=test
PORT=5001

# Test Database (MongoDB Memory Server will override this)
MONGODB_URI=mongodb://localhost:27017/racky-test

# JWT Configuration
JWT_SECRET=test-jwt-secret-key-for-testing-only
JWT_EXPIRES_IN=1h

# Client Configuration
CLIENT_URL=http://localhost:5173

# Disable external services in tests
STRIPE_SECRET_KEY=test_stripe_key
STRIPE_PUBLISHABLE_KEY=test_stripe_publishable_key
STRIPE_WEBHOOK_SECRET=test_webhook_secret

OPENAI_API_KEY=sk-test-openai-key-for-testing-only-1234567890

# Test-specific settings
LOG_LEVEL=silent
DISABLE_RATE_LIMITING=true
DISABLE_NOTIFICATIONS=true